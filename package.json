{
  "name": "schematik",
  "version": "1.1.0",
  "description": "Human-friendly utilities for building JSON schemas.",
  "main": "lib/index.js",
  "engines": {
    "node": "~0.12.2"
  },
  "scripts": {
    "test": "gulp test",
    "coverage": "mocha --require blanket --reporter html-cov > coverage.html",
    "postinstall": "./node_modules/.bin/gulp"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jluchiji/schematik"
  },
  "keywords": [
    "json",
    "schema",
    "js"
  ],
  "author": "Denis Luchkin-Zhou <wyvernzora@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jluchiji/schematik/issues"
  },
  "homepage": "https://github.com/jluchiji/schematik",
  "dependencies": {
    "debug": "^2.2.0",
    "gulp-istanbul": "^0.10.0",
    "gulp-mocha": "^2.1.3",
    "isparta": "^3.0.3",
    "lodash": "^3.9.3",
    "seamless-immutable": "^2.4.2"
  },
  "devDependencies": {
    "babel": "^5.6.23",
    "babel-runtime": "^5.7.0",
    "blanket": "^1.1.7",
    "chai": "^2.3.0",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.1.0",
    "gulp-jscs": "^1.6.0",
    "gulp-jscs-stylish": "^1.1.0",
    "gulp-jshint": "^1.11.2",
    "gulp-sourcemaps": "^1.5.2",
    "jshint-stylish": "^2.0.1",
    "mocha": "^2.2.5",
    "mocha-babel": "^3.0.0",
    "mocha-sinon": "^1.1.4",
    "sinon": "^1.15.4",
    "source-map-support": "^0.3.2"
  },
  "config": {
    "blanket": {
      "pattern": [
        "main.js",
        "lib"
      ],
      "data-cover-never": [
        "node_modules",
        "test"
      ]
    }
  }
}
